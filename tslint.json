{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "double"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "ordered-imports": false,
    "member-ordering": false,
    "object-literal-sort-keys": false,
    "member-access": [
      true,
      "no-public"
    ],
    "trailing-comma": true,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ],
    "max-line-length": [
      true,
      130
    ],
    "no-bitwise": false,
    "indent": [true, "spaces", 4]
  },
  "rulesDirectory": []
}
